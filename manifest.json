{
    "manifest_version": 3,
    "name": "Dark Pattern Detector",
    "version": "1.0",
    "description": "Detects dark patterns on websites using an API.",
    "permissions": ["activeTab", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
        
      }
    ]
  }
  